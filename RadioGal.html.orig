	function imgbar_init(num,news_num,contents_num){
		CurrentOrder=num;
		CurrentImgOrder=num;
		NumTxt=contents_num;
		NumImg=news_num;

		if(false){
			CurrentOrder = 12;
		}

		if(false && (img_imgbar_contents[CurrentImgOrder] == "\"dc_shopping\"" || img_imgbar_contents[CurrentOrder] == "\"dc_shopping\"")) {
			GEID('imgbar_basic').style.display  = 'none';
			GEID('imgbar_iframe').style.display = '';
		} else {
			GEID('imgbar_basic').style.display  = '';
			GEID('imgbar_iframe').style.display = 'none';
			GEID('imgbar_img').innerHTML = img_imgbar_contents[CurrentImgOrder];	
			GEID('imgbar_txt1').innerHTML = NewTxt_imgbar_contents[CurrentImgOrder];
			GEID('imgbar_txt2').innerHTML = txt_imgbar_contents[CurrentOrder];
		}
	}

	function imgbar_move(interval){

		NextTxtOrder=parseInt(CurrentOrder)+(parseInt(interval));
		NextImgOrder=parseInt(CurrentImgOrder)+parseInt(interval);
		
		if(NextImgOrder==parseInt(NumImg))CurrentImgOrder=0; else if(NextImgOrder=='-1')CurrentImgOrder=NumImg-1; else CurrentImgOrder=NextImgOrder;
		if(NextTxtOrder==parseInt(NumTxt))CurrentOrder=0; else if(NextTxtOrder=='-1')CurrentOrder=NumTxt-1; else CurrentOrder=NextTxtOrder;

		if(false && (img_imgbar_contents[CurrentImgOrder] == "\"dc_shopping\"" || img_imgbar_contents[CurrentOrder] == "\"dc_shopping\"")) {
			GEID('imgbar_basic').style.display  = 'none';
			GEID('imgbar_iframe').style.display = '';
		} else {
			GEID('imgbar_basic').style.display  = '';
			GEID('imgbar_iframe').style.display = 'none';
			GEID('imgbar_img').innerHTML = img_imgbar_contents[CurrentImgOrder];
			GEID('imgbar_txt1').innerHTML = NewTxt_imgbar_contents[CurrentImgOrder];
			GEID('imgbar_txt2').innerHTML = txt_imgbar_contents[CurrentOrder];
		}

		//if((CurrentOrder==contents_num||CurrentOrder+1==contents_num)&&interval>0)CurrentOrder=-2;
		//if((CurrentOrder==1||CurrentOrder==0)&&interval<0)CurrentOrder=contents_num;
	}

